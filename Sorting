#Sorting Descending order
arr=[7,3,12,5,65,73,1,9,6,41]

for i in range(len(arr)):
    for j in range(i+1,len(arr)):
        if arr[i]<arr[j]:
            arr[i],arr[j]=arr[j],arr[i]


print(arr)

--------------------------------------------------------------------
#Sorting Ascending order
arr=[7,3,12,5,65,73,1,9,6,41]

for i in range(len(arr)):
    for j in range(i+1,len(arr)):
        if arr[i]>arr[j]:
            arr[i],arr[j]=arr[j],arr[i]


print(arr)
